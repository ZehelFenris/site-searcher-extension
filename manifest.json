{
  "manifest_version": 3,
  "name": "Wiki Searcher",
  "version": "1.1",
  "description": "Search different wikis from a dropdown menu.",
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}